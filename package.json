{
  "name": "leaflet-iiif",
  "version": "3.0.0",
  "description": "A Leaflet plugin for viewing IIIF sources",
  "main": "build/leaflet-iiif.js",
  "typings": "build/index.d.ts",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "build": "npm run compile && webpack --progress && webpack --progress --mode production",
    "start": "webpack serve",
    "compile": "./node_modules/.bin/tsc",
    "test": "mocha --require jsdom-global/register --require ts-node/register ./test/unit/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mejackreed/Leaflet-IIIF.git"
  },
  "keywords": [
    "Leaflet",
    "IIIF",
    "map",
    "zoom"
  ],
  "author": "Jack Reed",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mejackreed/Leaflet-IIIF/issues"
  },
  "homepage": "https://github.com/mejackreed/Leaflet-IIIF",
  "devDependencies": {
    "@types/leaflet": "^1.7.0",
    "@types/mocha": "^8.2.2",
    "css-loader": "^5.2.1",
    "jsdom": "16.5.3",
    "jsdom-global": "3.0.2",
    "mocha": "^8.3.2",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^7.1.1",
    "ts-loader": "^8.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "webpack": "^5.31.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "leaflet": "^1.7.1"
  }
}
