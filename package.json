{
  "name": "leaflet-iiif",
  "version": "3.0.0",
  "description": "A Leaflet plugin for viewing IIIF sources",
  "main": "./dist/umd/index.min.js",
  "module": "./dist/esm/index.js",
  "typings": "./dist/typing/index.d.ts",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "clean": "rimraf dist/",
    "lint": "esw . --ext .ts",
    "compile": "tsc",
    "build": "npm run clean && npm run lint && npm run compile && webpack --progress && webpack --progress --mode production",
    "start": "npm run build  && concurrently \"npm run compile -- -w\" \"npm run lint -- -w\" \"webpack serve\" ",
    "test": "TS_NODE_PROJECT='./tsconfig.node.json' mocha --require jsdom-global/register --require ts-node/register ./test/unit/**/*.ts",
    "e2e:test": "TS_NODE_PROJECT='./tsconfig.node.json' mocha --require ts-node/register ./test/e2e/**/*.spec.ts --exit",
    "e2e:generate": "TS_NODE_PROJECT='./tsconfig.node.json' node -r ts-node/register test/e2e/generate.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mejackreed/Leaflet-IIIF.git"
  },
  "keywords": [
    "Leaflet",
    "IIIF",
    "map",
    "zoom"
  ],
  "author": "Jack Reed",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mejackreed/Leaflet-IIIF/issues"
  },
  "homepage": "https://github.com/mejackreed/Leaflet-IIIF",
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@types/leaflet": "^1.7.0",
    "@types/mocha": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.0.2",
    "css-loader": "^5.2.1",
    "eslint": "^7.24.0",
    "eslint-watch": "^7.0.0",
    "jsdom": "16.5.3",
    "jsdom-global": "3.0.2",
    "mini-css-extract-plugin": "^1.5.0",
    "mocha": "^8.3.2",
    "pixelmatch": "^5.2.1",
    "pngjs": "^6.0.0",
    "puppeteer": "^8.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^7.1.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "webpack": "^5.31.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "leaflet": "^1.7.1"
  }
}
